{
	"update_url": "http://clients2.google.com/service/update2/crx",
    "name": "Link Tester",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Link tester.",
	"permissions": [
	          "storage", "activeTab", "tabs", "webRequest", "http://*/*"
	        ],
	"background": {
		"scripts": ["background.js"],
		"persistent": true
	},
	"content_scripts": [
		{
			"matches": ["http://*/*"],
			"js": ["jquery-2.1.1.min.js", "content.js"]
		}
	]
}